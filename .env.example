# Copy this file to .env.local and replace the placeholder values.
GEMINI_API_KEY=your-gemini-api-key

# Firebase Web App configuration (Project settings > General > Web apps)
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=000000000000
VITE_FIREBASE_APP_ID=1:000000000000:web:abcdefghijklmnopqrstuvwxyz
# Optional: only if Google Analytics is enabled
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX
